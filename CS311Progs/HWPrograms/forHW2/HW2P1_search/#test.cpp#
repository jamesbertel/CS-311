#include <iostream>
using namespace std;

int main()
{
  int A[10];

  for(int i = 0; i < 10; i++)
    {
      A[i] = i+1;
      cout << "A[" << i << "] = " << A[i] << endl;
    }


  int first = A[0];
  int last = A[9];

  cout << first << " " << last << endl;

  int middle = int((first+last)/2);

  cout << middle << endl;

  return 0;
}
